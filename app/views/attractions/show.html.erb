<%= form_tag "/rides/new", method: :post do %>

  Name: <%= @attraction.name %><br/>
  Total Number of Users: <%= @attraction.rides.size %><br/>
  Ticket Requirement: <%= @attraction.tickets %><br/>
  Height Requirement: <%= @attraction.name %><br/>
  Nausea Rating: <%= @attraction.name %><br/>
  Happiness Rating: <%= @attraction.name %><br/>

  <%= submit_tag("Go on this ride") %>

<% end %>


<%= form_for @attraction.rides.build do |f| %>



  <%= f.submit %>

<% end %>
